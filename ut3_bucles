#Programa para hallar los multiplos de 3 hasta un limite. 
def run(limit: int) -> None:
    num = 0
    new_value = num

    while new_value < limit:
        print(num, end=' ')
        num += 3
        new_value += num
    print(num)


# DO NOT TOUCH THE CODE BELOW
if __name__ == '__main__':
    import vendor

    vendor.launch(run)
